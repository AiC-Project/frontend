{
  "name": "front-end-app",
  "version": "0.3.0",
  "description": "Front-end application for Android in Cloud project.",
  "repository": {
    "type": "git",
    "url": "git@git.android-in-cloud.com:aic_vm/front-end-app.git"
  },
  "scripts": {
    "test": "NODE_PATH=./src ava --tap | tap-nyan",
    "xo": "xo",
    "test-verbose": "NODE_PATH=./src ava",
    "coverage": "nyc --extension .jsx npm run test && nyc report --reporter=lcov",
    "oldstart": "gulp watch",
    "oldbuild": "gulp build",
    "start": "webpack-dev-server --content-base ./build/ --history-api-fallback --progress --port 9080 --hot --inline",
    "webpack-build": "webpack --progress",
    "copy-static": "cp -R src/www/* build/",
    "babel-helpers": "babel-external-helpers > build/babel-helpers.js",
    "build": "npm run babel-helpers && npm run copy-static && npm run webpack-build",
    "create-conf-from-env": "gulp create-conf-from-env",
    "no-console": "echo \"Verifying for console usage. Please use debug package instead.\" && ! grep -rnw 'src' -e \"console\" && echo \"Yes! No console.something being used! Good Job!\"",
    "no-todos": "echo \"Verifying for TODOs.\" && ! grep -rnw 'src' -e \"TODO\" && echo \"Yes! No TODOs to do! Good Job!\""
  },
  "author": "Karine Pires",
  "dependencies": {
    "attr-accept": "^1.0.3",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.12.3",
    "browserify": "^13.0.0",
    "debug": "^2.2.0",
    "deep-extend": "^0.4.1",
    "envify": "^3.4.0",
    "formsy-react": "^0.18.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-less": "^3.0.5",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "material-ui": "0.15.0-beta.1",
    "mdi": "^1.5.54",
    "noVNC": "git+https://github.com/kanaka/noVNC.git#v0.5.1",
    "pretty-hrtime": "^1.0.2",
    "promise-polyfill": "^4.0.3",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-dropzone": "^3.4.0",
    "react-router": "^2.2.4",
    "react-tap-event-plugin": "1.0.0",
    "reflux": "^0.4.1",
    "reflux-core": "^0.3.0",
    "reflux-promise": "^1.0.4",
    "require-dir": "^0.3.0",
    "sanitize-html": "^1.11.4",
    "schema-inspector": "^1.6.4",
    "string": "^3.3.1",
    "underscore": "^1.8.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack": "^2.1.0-beta.7"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-xo-react": "^0.6.0",
    "eslint-plugin-react": "^5.0.1",
    "http-server": "^0.9.0",
    "nyc": "^6.4.0",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-perf": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "tap-nyan": "0.0.2",
    "xo": "^0.14.0",
    "webpack-dev-server": "^2.0.0-beta"
  },
  "config": {
    "loglevel": "warn"
  },
  "xo": {
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "build/**/*"
    ]
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "require": "babel-core/register",
    "files": [
      "test/**/*.js",
      "!test/unit/utils/*.js"
    ]
  },
  "nyc": {
    "extensions": [
      ".jsx"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015-native-modules",
      "stage-0"
    ],
    "sourceMaps": "both"
  }
}
